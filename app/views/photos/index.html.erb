<h1>Listing photos</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.name %></td>
    <td><%= link_to 'Show', [@mem, photo] %></td>
    <td><%= link_to 'Edit', edit: [@mem, photo] %></td>
    <td><%= link_to 'Destroy', [@mem, photo], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_mem_photo_path(@mem) %>
